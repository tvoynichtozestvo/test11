<Window x:Class="WpfApp2.CalculationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="CalculationWindow" FontFamily="Candara" Height="450" Width="318">
    <Grid>
        <TextBox x:Name="CountBox" HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="266" Height="26" Margin="0,203,0,0"/>
        <ComboBox x:Name="TypeBox" HorizontalAlignment="Center" Margin="0,152,0,0" VerticalAlignment="Top" Width="266" Height="26"/>
        <TextBox x:Name="param2Box" HorizontalAlignment="Center" Margin="0,311,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="266" Height="26"/>
        <TextBox x:Name="param1Box" HorizontalAlignment="Center" Margin="0,259,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="266" Height="26"/>
        <ComboBox x:Name="MaterialBox" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top" Width="266" Height="26"/>
        <Button Background="#355CBD" Foreground="White" x:Name="EditBtn" Content="Расчет" HorizontalAlignment="Center" Margin="0,365,0,0" VerticalAlignment="Top" Width="266" Height="39" Click="EditBtn_Click"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,43,0,0" TextWrapping="Wrap" Text="Расчет кол-ва продукции" VerticalAlignment="Top" FontSize="18"/>

    </Grid>
</Window>
